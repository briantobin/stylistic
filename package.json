  },
  "homepage": "https://github.com/steadicat/stylistic",
  "repository": {
    "type": "git",
    "url": "git@github.com:steadicat/stylistic.git"
  },
  "bugs": {
    "url": "https://github.com/steadicat/stylistic/issues"
  },
  "keywords": [
    "react",
    "styles",
    "style",
    "css"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": ">6.0.0-beta",
    "babel-preset-es2015-rollup": "1.1.x",
    "eslint": "2.4.x",
    "eslint-plugin-babel": "3.1.x",
    "rollup": "0.25.x",
    "rollup-plugin-babel": "2.4.x"
  },
  "license": "MIT"
